    weight_filler {
      type: "msra"
      #type: "gaussian" #"msra"
      #std: 0.001
    }
  }
}
layer {
  name: "concat_2_3"
  type: "Concat"
  bottom: "concat_2_2"
  bottom: "conv2_3/x2"
  top: "concat_2_3"
}


layer {
  name: "conv2_blk/bn"
  type: "BatchNorm"
  bottom: "concat_2_3"
  top: "conv2_blk/bn"
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
}
layer {
  name: "conv2_blk/scale"
  type: "Scale"
  bottom: "conv2_blk/bn"
  top: "conv2_blk/bn"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  scale_param {
    bias_term: true
  }
}
layer {
  name: "relu2_blk"
  type: "ReLU"
  bottom: "conv2_blk/bn"
  top: "conv2_blk/bn"
}
layer {
	bottom: "conv2_blk/bn"
	top: "conv5_dcpp_1-sm"
	name: "conv5_dcpp_1-sm"
	param {
		lr_mult: 1
	    decay_mult: 1
	}
	param {
		lr_mult: 2
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 2
		pad: 1
    	dilation: 1
		kernel_size: 3
		weight_filler {
      		#type: "gaussian"
                type: "msra"
	    	}
	    	bias_filler {
			type: "constant"
     			 value: 0
		}
	}
}
layer {
	bottom: "conv5_dcpp_1-sm"
	top: "conv5_dcpp_1-sm"
	name: "relu5_dcpp_1-sm"
	type: "ReLU"
}

layer {
	bottom: "conv2_blk/bn"
	top: "conv5_dcpp_3-sm"
	name: "conv5_dcpp_3-sm"
	param {
		lr_mult: 1
	    decay_mult: 1
	}
	param {
		lr_mult: 2
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 2
		pad: 3
    	dilation: 3
		kernel_size: 3
		weight_filler {
      		#type: "gaussian"
                type: "msra"
	    	}
	    	bias_filler {
			type: "constant"
     			 value: 0
		}
	}
