	bottom: "conv2_blk/bn"
	top: "conv5_dcpp_5-sm"
	name: "conv5_dcpp_5-sm"
	param {
		lr_mult: 1
	    decay_mult: 1
	}
	param {
		lr_mult: 2
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 2
		pad: 5
    	dilation: 5
		kernel_size: 3
		weight_filler {
      		#type: "gaussian"
                type: "msra"
	    	}
	    	bias_filler {
			type: "constant"
     			 value: 0
		}
	}
}
layer {
	bottom: "conv5_dcpp_5-sm"
	top: "conv5_dcpp_5-sm"
	name: "relu5_dcpp_5-sm"
	type: "ReLU"
}
layer {
	bottom: "conv2_blk/bn"
	top: "conv5_dcpp_7-sm"
	name: "conv5_dcpp_7-sm"
	param {
		lr_mult: 1
	    decay_mult: 1
	}
	param {
		lr_mult: 2
	    decay_mult: 0
	}
	type: "Convolution"
	convolution_param {
		num_output: 2
		pad: 7
    	dilation: 7
		kernel_size: 3
		weight_filler {
      		#type: "gaussian"
                type: "msra"
	    	}
	    	bias_filler {
			type: "constant"
     			 value: 0
		}
	}
}
layer {
	bottom: "conv5_dcpp_7-sm"
	top: "conv5_dcpp_7-sm"
	name: "relu5_dcpp_7-sm"
	type: "ReLU"
}
layer {
	bottom: "conv5_dcpp_1-sm"
	bottom: "conv5_dcpp_3-sm"
	bottom: "conv5_dcpp_5-sm"
	bottom: "conv5_dcpp_7-sm"
	top: "concat_conv5_dcpp"
	name: "concat_conv5_dcpp"
	type: "Concat"
}
layer {
  name: "final_score1024"
  type: "Convolution"
  bottom: "concat_conv5_dcpp"
  top: "final_score1024"
  param {
    lr_mult: 1 #10000
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 2
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "msra"
      #type: "gaussian" #"msra"
      #std: 0.001
    }
  #  bias_term: false
    bias_filler {
     type: "constant"
      value: 0
    }
  }
}




